<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildProjectDirectory)\getversion.proj" />
  <ItemGroup>
    <Binaries Include="Versioner\"/>
  </ItemGroup>
  <Target Name="Build">
    <Exec IgnoreExitCode="true" Command="Versioner.exe --inpath %(Binaries.RelativeDir) --major $(Major) --minor $(Minor) --build $(Build) --revision 0 --trademark $(Revision)"  />
  </Target>
</Project>
